<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_form_inherit" model="ir.ui.view">
        <field name="name">res.config.settings.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="industry_fsm.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='module_industry_fsm_report']" position="after">
                <div class="col-12 col-lg-6 o_setting_box" name="module_dispatch_requests">
                    <div class="o_setting_left_pane">
                        <field name="horas_diferencias"/>
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="horas_diferencias"/>
                        <div class="text-muted">
                            Horas de diferencias
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>